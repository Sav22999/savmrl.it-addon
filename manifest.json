{
  "manifest_version": 3,
  "name": "savmrl.it",
  "version": "1.0",
  "description": "An anonymous and free shortener link and redirect service.",
  "permissions": [
    "tabs"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": {
      "16": "/images/icon.png",
      "48": "/images/icon.png",
      "128": "/images/icon.png"
    },
    "default_title": "savmrl.it"
  }
}